---
  import { db, GalleryImage } from 'astro:db';
  import Carousel from '@src/components/common/Carousel.astro';

  const imagesUrl = (
    await db
    .select({ path: GalleryImage.path })
    .from(GalleryImage)
    .run()
  )
  .rows
  .map(image => `${Astro.url.origin}/${image.path}`);
---

<Carousel images={imagesUrl} />
