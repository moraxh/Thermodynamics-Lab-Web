---
	const { title, class: className, ...rest } = Astro.props;

	import Empty from "@layouts/Empty.astro"
	import AdminTopBar from "@src/components/common/AdminTopBar.astro";
---

<Empty title={title}>
	<AdminTopBar />
	<main class:list={[
		"flex flex-col justify-start items-center gap-12 mx-3 sm:mx-10 h-full my-auto p-2 sm:p-10",
		className
		]}>
		<slot />
	</main>
</Empty>
